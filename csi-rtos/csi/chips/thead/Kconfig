choice
depends on CHIP_PANGU
prompt "pangu cpu select"
     default CHIP_PANGU_CPU0
     help
       select the pangu cpu

config CHIP_PANGU_CPU0
     bool "pangu_cpu0"
     depends on CHIP_VENDOR_THEAD
     help
       select the chip type is pangu

config CHIP_PANGU_CPU1
     bool "pangu_cpu1"
     depends on CHIP_VENDOR_THEAD
     help
       select the chip type is pangu

config CHIP_PANGU_CPU2
     bool "pangu_cpu2"
     depends on CHIP_VENDOR_THEAD
     help
       select the chip type is pangu
endchoice

config CHIP_NAME_STR
    string
    default "smartl"       if CHIP_SMARTL
    default "smarth_mmu"   if CHIP_SMARTH_MMU
    default "smarth_nommu" if CHIP_SMARTH_NOMMU
    default "smarth_610"   if CHIP_SMARTH_610
    default "smarth_610m"  if CHIP_SMARTH_610M
    default "smartl_rv32"  if CHIP_SMARTL_RV32
    default "smartl_ck802" if CHIP_SMARTL_CK802
    default "smartl_ck803" if CHIP_SMARTL_CK803
    default "smartl_ck804f" if CHIP_SMARTL_CK804F
    default "smartl_ck804ef" if CHIP_SMARTL_CK804EF
    default "smartl_ck805f" if CHIP_SMARTL_CK805F
    default "ch2201"       if CHIP_CH2201
    default "phobos"       if CHIP_PHOBOS
    default "tx216"        if CHIP_TX216
    default "ch2301"       if CHIP_CH2301
    default "ch6101"       if CHIP_CH6101
    default "hobbit4"      if CHIP_HOBBIT4
    default "jupiter_bc"   if CHIP_JUPITER_BC
    default "jupiter_lc"   if CHIP_JUPITER_LC
    default "jupiter_bc_mpw_1"   if CHIP_JUPITER_BC_MPW_1
    default "jupiter_lc_mpw_1"   if CHIP_JUPITER_LC_MPW_1
    default "jupiter_bc_mpw_2"   if CHIP_JUPITER_BC_MPW_2
    default "jupiter_lc_mpw_2"   if CHIP_JUPITER_LC_MPW_2
    default "sparrow"      if CHIP_SPARROW
    default "light"        if CHIP_LIGHT
    default "light_fm"     if CHIP_LIGHT_FM
    default "light_aon"    if CHIP_LIGHT_AON
    default "avfs"         if CHIP_AVFS
    default "yunvoice"     if CHIP_YUNVOICE
    default "pangu_cpu0"   if CHIP_PANGU_CPU0
    default "pangu_cpu1"   if CHIP_PANGU_CPU1
    default "pangu_cpu2"   if CHIP_PANGU_CPU2
    default "danica"       if CHIP_DANICA
    default "alkaid"       if CHIP_ALKAID
    default "mizar"        if CHIP_MIZAR
    default "merak"        if CHIP_MERAK
    default "dubhe"        if CHIP_DUBHE
    default "hobbit3_se"   if CHIP_HOBBIT3_SE
    default "eragon2_r807" if CHIP_ERAGON2_R807
    default "eragon3"      if CHIP_ERAGON3
    default "cetus"        if CHIP_CETUS
    default "ln8819_cpu0"  if CHIP_LN8819_CPU0 
    default "ln8819_cpu1"  if CHIP_LN8819_CPU1
    default "aries_se"     if CHIP_ARIES_SE
    default "aries_ap"     if CHIP_ARIES_AP
    default "wujian100_open"   if CHIP_WUJIAN100_OPEN
    default "simulate"     if CHIP_SIMULATE
