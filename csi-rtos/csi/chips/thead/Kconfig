choice
     prompt "chip select"
     default CHIP_PHOBOS
     help
       select the chip

config CHIP_SMARTL
     bool "smartl"
     depends on CHIP_VENDOR_THEAD
     select HAVE_VIC
     help
       select the chip type is smart-l01

config CHIP_SMARTH_MMU
     bool "smarth mmu"
     depends on CHIP_VENDOR_THEAD && (CPU_C807 || CPU_C807F || CPU_C807FV || CPU_C810 || CPU_C810V)
     help
       select the chip type is smarth with mmu

config CHIP_SMARTH_610
     bool "smarth610"
     depends on CHIP_VENDOR_THEAD && (CPU_CK610E || CPU_CK610EF)
     help
       select the chip type is smarth610

