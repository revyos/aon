choice
     prompt "coretim options"
     default CSKY_CORETIM
     help
         select the coretim type

config CSKY_CORETIM
    bool "csky coretim"
    help
       coretim of csky

config RV32_CORETIM
    bool "rv32"
    help
       coretim of rv32

endchoice

config SYSTEM_SECURE
    bool "security system"
    help
      select the security system

config XIP
    bool "xip"
    help
      select the run in flash

config MMU
    bool "mmu"
    help
      select the run in mmu
