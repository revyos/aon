choice
     prompt "arch options"
     default ARCH_CSKY
     help
       select the architecture

config ARCH_CSKY
     bool "csky"
     help
       csky architecture

config ARCH_RV32
     bool "rv32"
     help
       rv32 architecture

config ARCH_RV64
     bool "rv64"
     help
       rv64 architecture

config ARCH_ARM
     bool "arm"
     help
       arm architecture

endchoice

